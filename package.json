{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run apollo\" --names \"Nuxt,Apollo\" --prefix-colors \"cyan,magenta\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma db reset",
    "apollo": "node --loader ts-node/esm server/index.ts",
    "start:backend": "./scripts/start-backend.sh",
    "start:backend:pm2": "dotenv -e .env -- pm2 start ecosystem.config.js",
    "stop:backend:pm2": "pm2 stop loterie-backend",
    "restart:backend:pm2": "pm2 restart loterie-backend",
    "migrate:sqlite-to-postgres": "node --loader ts-node/esm scripts/migrate-data-prisma.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.14.0",
    "@apollo/server": "^5.0.0",
    "@prisma/client": "^5.10.2",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.2",
    "@vue/apollo-composable": "^4.2.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "h3": "^1.15.4",
    "jsonwebtoken": "^9.0.2",
    "node-mailjet": "^6.0.6",
    "nodemailer": "^7.0.9",
    "nuxt": "^3.14.1592",
    "rxjs": "^7.8.2",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.3.1",
    "better-sqlite3": "^12.4.6",
    "concurrently": "^9.2.1",
    "prettier": "^3.3.3",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
